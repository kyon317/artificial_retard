int printBST(BTnode *tree, int counter) 
{
	if (counter>=5)
	{
		return counter;
	}
	if (tree == NULL)
	{
		return counter;
	}
	if (tree->right == NULL)
	{
		cout << tree->key << " ";
		counter++;
		return counter;
	}
	else
	{
		counter=printBST(tree->right, counter);
		if (counter >= 5)
		{
			return counter;
		}
		cout << tree->key << " ";
		counter++;
		counter=printBST(tree->left, counter);
	}
}
